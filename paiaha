repeat wait() until game:IsLoaded()
local Venyx = loadstring(game:HttpGet("https://raw.githubusercontent.com/pyro-mods/Venyx-UI/main/source.lua"))()

local window = Venyx.new("AnhTuấnDzai Hub - Blox Fruits", 5013109572)

game.StarterGui:SetCore("SendNotification", {
    Title = "AnhTuấnDzai Hub";
    Text = "Welcome to Tuấn Anh Roblox Community";
    Duration = 5;
})

local function AutoJoinMarines()
    pcall(function()
        if game.Players.LocalPlayer.Team ~= game.Teams.Marines then
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("SetTeam", "Marines")
        end
    end)
end
AutoJoinMarines()

local vu = game:GetService("VirtualUser")
game.Players.LocalPlayer.Idled:Connect(function()
    vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    wait(1)
    vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

local farmTab = window:Tab("Auto Farm")
farmTab:Toggle("Auto Farm Level", false, function(state)
    getgenv().AutoFarm = state
    while getgenv().AutoFarm do
        pcall(function()
            local player = game.Players.LocalPlayer
            local humanoid = player.Character:WaitForChild("HumanoidRootPart")
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("StartQuest", "BanditQuest1", 1)
            for i,v in pairs(workspace.Enemies:GetChildren()) do
                if v.Name == "Bandit" and v:FindFirstChild("HumanoidRootPart") then
                    humanoid.CFrame = v.HumanoidRootPart.CFrame + Vector3.new(0, 5, 0)
                    game.ReplicatedStorage.Remotes.CommF_:InvokeServer("Attack", v)
                end
            end
        end)
        wait(0.1)
    end
end)

local tpTab = window:Tab("Teleport")
tpTab:Button("Teleport to Starter Island", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-6551, 8, 1446)
end)
tpTab:Button("Teleport to Marine Fortress", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-5035, 73, 4325)
end)

local espTab = window:Tab("ESP")
local function createESP(object, text)
    if object:FindFirstChild("ESP") then return end
    local billboard = Instance.new("BillboardGui", object)
    billboard.Name = "ESP"
    billboard.AlwaysOnTop = true
    billboard.Size = UDim2.new(1, 0, 1, 0)

    local label = Instance.new("TextLabel", billboard)
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.fromRGB(255, 0, 0)
    label.TextStrokeTransparency = 0
    label.TextScaled = true
end

espTab:Toggle("ESP Players", false, function(state)
    getgenv().ESPPlayers = state
    while getgenv().ESPPlayers do
        for _, player in pairs(game.Players:GetPlayers()) do
            if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                createESP(player.Character.HumanoidRootPart, player.Name)
            end
        end
        wait(1)
    end
end)

espTab:Toggle("ESP Chests", false, function(state)
    getgenv().ESPChests = state
    while getgenv().ESPChests do
        for _, chest in pairs(workspace:GetChildren()) do
            if chest:IsA("Model") and chest.Name:lower():find("chest") then
                createESP(chest.PrimaryPart or chest:FindFirstChild("HumanoidRootPart"), "Chest")
            end
        end
        wait(1)
    end
end)

local utilTab = window:Tab("Utilities")
utilTab:Button("Reset Character", function()
    game.Players.LocalPlayer.Character:BreakJoints()
end)
utilTab:Button("Server Hop", function()
    local Http = game:GetService("HttpService")
    local TPS = game:GetService("TeleportService")
    local ApiUrl = "https://games.roblox.com/v1/games/2753915549/servers/Public?sortOrder=Asc&limit=100"

    local Servers = Http:JSONDecode(game:HttpGet(ApiUrl)).data
    for i,server in pairs(Servers) do
        if server.playing < server.maxPlayers then
            TPS:TeleportToPlaceInstance(game.PlaceId, server.id, game.Players.LocalPlayer)
            break
        end
    end
end)

window:SelectTab(window.tabs[1])
