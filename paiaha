local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "DeathXUltimate", HidePremium = false, IntroText = "SyntaXDeath Scripts Hub V1", SaveConfig = true, ConfigFolder = "Scripts"})

local TabBloxFruits = Window:MakeTab({
    Name = "Blox Fruits",
    Icon = "https://img.icons8.com/plasticine/2x/fruits.png",
    PremiumOnly = false
})

OrionLib:MakeNotification({
    Name = "HEY",
    Content = "linktr.ee/codeweb.com !",
    Image = "rbxassetid://4483345998",
    Time = 5
})

local SectionBloxFruits = TabBloxFruits:AddSection({
    Name = "Blox Fruits Scripts"
})

-- Add Blox Fruits scripts
TabBloxFruits:AddButton({
    Name = "Auto Farm Script",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Synthetic6969/Blox-Fruits-Script/main/AutoFarm.lua"))()
        print("Auto Farm script pressed")
    end
})

TabBloxFruits:AddButton({
    Name = "Teleport Script",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Synthetic6969/Blox-Fruits-Script/main/Teleport.lua"))()
        print("Teleport script pressed")
    end
})

TabBloxFruits:AddButton({
    Name = "Devil Fruits Unlocker",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Synthetic6969/Blox-Fruits-Script/main/DevilFruitUnlocker.lua"))()
        print("Devil Fruits Unlocker script pressed")
    end
})

